<h1>Events you have attended</h1>

<div>
  <table>
    <% @attended_events.each do |event| %>
      <tr>
        <td><%= link_to event.title, event_path(event) %> | </td>
        <td>
          <%= form_for(@rating, url: "/events/#{event.id}/rate") do |f| %>
            <%= f.label :stars %>
            <%= f.select :stars, (1..5) %>
            <%= f.submit "Rate this event!" %>
          <% end %>
      </tr>
    <% end %>
  </table>
</div>

